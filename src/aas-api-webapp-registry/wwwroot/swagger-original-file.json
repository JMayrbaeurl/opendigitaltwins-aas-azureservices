            "items" : {
              "$ref" : "#/components/schemas/Identifier"
            }
          },
          "packageId" : {
            "type" : "string"
          }
        },
        "example" : {
          "aasIds" : [ "aasIds", "aasIds" ],
          "packageId" : "packageId"
        }
      },
      "packages_body" : {
        "type" : "object",
        "properties" : {
          "aasIds" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Identifier"
            }
          },
          "file" : {
            "type" : "string",
            "format" : "binary"
          },
          "fileName" : {
            "type" : "string"
          }
        }
      },
      "packages_packageId_body" : {
        "type" : "object",
        "properties" : {
          "aasIds" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Identifier"
            }
          },
          "file" : {
            "type" : "string",
            "format" : "binary"
          },
          "fileName" : {
            "type" : "string"
          }
        }
      }
    }
  }
}
